{
    "$schema": "https://schema.tauri.app/config/2",
    "productName": "client-ai",
    "version": "1.0.0",
    "identifier": "com.node.client-ai",
    "app": {
        "withGlobalTauri": true,
        "windows": [
            {
                "title": "Client ai",
                "width": 450,
                "height": 600,
                "theme": "Dark"
            }
        ],
        "security": {
            "csp": null
        }
    },
    "build": {
        "frontendDist": "../public",
        "devUrl": "http://localhost:30000",
        "beforeDevCommand": "IS_DEPLOY_DEV=true npm run execute",
        "beforeBuildCommand": "IS_DEPLOY_DEV=false npm run build"
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
        ]
    }
}
